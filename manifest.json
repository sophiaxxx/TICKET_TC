{
  "manifest_version": 2,
  "name": "My Extension",
  "description": "Èôêtixcraft",
  "version": "1.0.0",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["https://tixcraft.com/*"],
      "js": ["execute.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}

